{
  "extends": "standard",
  "env": {
    "node": true,
    "es6": true
  },
  "rules": {
    "generator-star-spacing": 0,
    "radix": [2],
    "eol-last": [1],
    "new-cap": [0],
    "quotes": [2, "single"],
    "no-unused-vars": ["error"],
    "no-undef": [2],
    "dot-notation": [0],
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "key-spacing": [2],
    "space-infix-ops": [2],
    "no-redeclare": [2],
    "no-multi-spaces": [2],
    "comma-dangle": [2],
    "comma-spacing": [1],
    "padded-blocks": [0],
    "arrow-parens": [2, "as-needed"],
    "keyword-spacing": [2, {"overrides": {
          "catch": {"after": false}
      }}],
    "space-before-function-paren": [2, "never"],
    "camelcase": [0],
    "eqeqeq": [2],
    "one-var": [0],
    "strict": [2, "global"],
    "semi-spacing": [1],
    "semi": [2, "always"],
    "spaced-comment": [0],
    "no-extra-boolean-cast": [2],
    "no-shadow": [1],
    "handle-callback-err": [1],
    "no-trailing-spaces": [0],
    "no-var": [2],
    "yoda": [1],
    "no-empty": [1]
  }
}
